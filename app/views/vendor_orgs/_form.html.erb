<%= form_for(@vendor_org,:html=>{:class=>'form'}) do |f| %>
  <div class="group">
    <%= f.label :name ,'名称', :class=>'label'%>
    <%= f.text_field :name , :class=>'text_field' %>
  </div>

  <div class="group navform wat-cf">
    <button class="button" type="submit">
      提交
    </button>
    <span class="text_button_padding">或</span>
    <%= link_to '取消', vendor_orgs_path,:class=>'text_button_padding link_button' %>
  </div>

<% end %>
