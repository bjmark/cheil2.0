<%= form_for(@item,:url=>@action_to) do |f| %>
  <div>名称:<%=@item.name%></div>
  <div>数量:<%=@item.quantity%></div>
  <div class="field">
    <%= f.label :price,'价格' %><br />
    <%= f.text_field :price %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
<p><%=link_to '返回',vendor_show_brief_path(@brief)%></p>
